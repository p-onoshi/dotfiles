# fish abbreviations
abbr -a aba 'abbr -a'
abbr -a abp 'abbr | peco'
abbr -a abs 'abbr -s'
abbr -a bri 'brew install'
abbr -a brud 'brew update'
abbr -a brug 'brew upgrade'
abbr -a cat bat
abbr -a dc docker-compose
abbr -a dcd 'docker-compose down'
abbr -a dcs 'docker-compose stop'
abbr -a dcu 'docker-compose up -d'
abbr -a gco 'git checkout'
abbr -a gcob 'git checkout -b'
abbr -a his history
abbr -a hisp 'history | peco'
abbr -a ll 'exa -abghHl'
abbr -a ls exa
abbr -a ps procs
abbr -a psp 'procs | peco'
abbr -a py python
abbr -a sfishc 'source ~/.config/fish/config.fish'
abbr -a vfishc 'vim ~/.config/fish/config.fish'